{
  "name": "Chrome Tools",
  "version": "1.1",
  "description": "Various tools to help with organization and time management",
  "permissions": ["webRequest","webRequestBlocking","<all_urls>","alarms","storage"],
  "background": {
    "page": "background/background.html"
  },
  "browser_action": {
      "default_popup": "browserAction/browserAction.html",
      "default_title": "Chrome Tools"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Q"
      }
    },
    "open_Schedule": {
      "suggested_key": {
        "default": "Alt+R"
      },
      "description": "Open Schedule"
    }
  },
  "options_page": "options.html",
  "web_accessible_resources": ["*"],
  "manifest_version": 2
}