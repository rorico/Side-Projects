{
  "name": "SiteBlocker",
  "version": "1.0",
  "description": "Time",
  "permissions": ["webRequest", "webRequestBlocking",
                  "<all_urls>","tabs"],
  "content_scripts": [ {
      "all_frames": true,
      "js": ["jquery.min.js","contentScript.js" ],
      "matches": [ "<all_urls>" ]
   } ],
   "browser_action": {
      "default_popup": "browserAction.html",
      "default_title": "SiteBlocker"
   },
  "background": {
    "scripts": ["scheduleInfo.js","background.js"]
  },
  "web_accessible_resources": ["*"],

  "manifest_version": 2
}
